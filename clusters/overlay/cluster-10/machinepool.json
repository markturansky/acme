{
  "apiVersion": "hive.openshift.io/v1",
  "kind": "MachinePool",
  "metadata": {
    "name": "cluster-10-worker",
    "namespace": "cluster-10"
  },
  "spec": {
    "clusterDeploymentRef": {
      "name": "cluster-10"
    },
    "name": "worker",
    "platform": {
      "aws": {
        "rootVolume": {
          "iops": 2000,
          "size": 100,
          "type": "io1"
        },
        "type": "t3a.xlarge",
        "zones": [
          "us-east-1c"
        ]
      }
    },
    "replicas": 1
  }
}